;\n  top: 0;\n  width: 65%;\n  margin: 0 auto;\n  padding: 15px;\n  border-bottom: 3px dotted black;\n  background: #feffd0;\n  font-size: 3rem;\n}\n\n/* a {\n  color: rgba(212, 114, 140) !important;\n  font-weight: 500;\n} */\n\n/* #region Coding Rules & Assumptions (\"CRs&As\") -- Coding Rules */\n\narticle#coding_rules {\n}\n\ndiv#container--coding_rules {\n  display: flex;\n  flex-direction: column;\n  flex-wrap: wrap;\n  justify-content: center;\n  align-items: center;\n  max-width: 100%;\n  margin: 3rem 0;\n}\n\ndiv#container--coding_rules div.coding_rule {\n  display: grid;\n  grid-template-columns: repeat (20, 0.05fr);\n  grid-template-columns: repeat(20, 5%);\n  /* grid-template-areas: \"rule_number rule_text rule_attribution\n    explanatory_text explanatory_text in_action\";\n     */\n  grid-template-rows: repeat(3, 0.25fr);\n  grid-template-rows: repeat(3, 25%);\n  margin: 2vh 0;\n  border: 1px dotted black;\n  width: 55%;\n  background-color: #ffffff;\n}\n\ndiv.coding_rule img {\n  max-width: 20px;\n}\n\ndiv.coding_rule span.rule_number {\n  display: grid;\n  justify-self: start;\n  align-self: center;\n  place-items: center;\n  grid-column: 1 / 10;\n  grid-row: 1;\n  min-width: 35px;\n  min-height: 35px;\n  margin-top: 5px;\n  margin-left: 15px;\n  background: black;\n  color: white;\n  border-radius: 50%;\n  font-size: 0.9rem;\n  /* grid-area: rule_number; */\n}\n\ndiv.coding_rule p.rule_text {\n  grid-column: 4 / 18;\n  grid-row: 2;\n  /* justify-self: center; */\n  max-width: 50ch;\n  margin: 35px 0px;\n  border-radius: 15px;\n  padding: 15px;\n  background: rgba(173, 216, 230, 0.5);\n  font-size: 1.25rem;\n  font-weight: 700;\n  /* grid-area: rule_text; */\n}\n\ndiv.coding_rule span.rule_attribution {\n  text-align: right;\n  grid-column: 11/21;\n  grid-row: 1;\n  /* grid-area: rule_attribution; */\n  width: 100%;\n  margin-right: 15px;\n  font-size: 0.95rem;\n}\n\ndiv.coding_rule div.rule_justifications {\n  grid-column: 1/10;\n  grid-row: 3;\n}\n\ndiv.coding_rule div.rule_justifications span {\n  display: grid;\n  place-items: center;\n}\ndiv.coding_rule div.rule_justifications li {\n  font-size: 0.75rem;\n  max-width: 45ch;\n}\n\ndiv.coding_rule div.rule_concrete_applications {\n  display: grid;\n  place-items: center;\n  grid-column: 11/21;\n  grid-row: 3;\n}\n\ndiv.coding_rule div.rule_concrete_applications span {\n}\n\ndiv.coding_rule div.rule_concrete_applications li {\n  font-size: 0.75rem;\n  max-width: 45ch;\n}\n\n/* #endregion CRs&As -- Coding Rules */\n\n/* #region CRs&As -- Coding Assumptions */\n\narticle#coding_assumptions {\n}\n\ndiv#container--coding_assumptions {\n  display: flex;\n  flex-direction: column;\n  flex-wrap: wrap;\n  justify-content: center;\n  align-items: center;\n  max-width: 100%;\n  margin: 3rem 0;\n}\n\ndiv#container--coding_assumptions div.coding_assumption {\n  display: grid;\n  grid-template-columns: repeat(20, 5%);\n  /* grid-template-areas: \"rule_number rule_text rule_attribution\n    explanatory_text explanatory_text in_action\";\n     */\n  grid-template-rows: repeat(3, 25%);\n  margin: 2vh 0;\n  border: 1px dotted black;\n  width: 55%;\n  background-color: #ffffff;\n}\n\ndiv.coding_assumption img {\n  max-width: 20px;\n}\n\ndiv.coding_assumption span.assumption_number {\n  display: grid;\n  justify-self: start;\n  align-self: center;\n  place-items: center;\n  grid-column: 1 / 10;\n  grid-row: 1;\n  min-width: 35px;\n  min-height: 35px;\n  margin-top: 5px;\n  margin-left: 15px;\n  background: black;\n  color: white;\n  border-radius: 50%;\n  font-size: 0.9rem;\n  /* grid-area: rule_number; */\n}\n\ndiv.coding_assumption p.assumption_text {\n  grid-column: 3 / 18;\n  grid-row: 2;\n  /* justify-self: center; */\n  max-width: 50ch;\n  margin: 35px 0px;\n  padding: 5px;\n  background: rgba(173, 216, 230, 0.5);\n\n  font-size: 1.25rem;\n  font-weight: 700;\n  /* grid-area: rule_text; */\n}\n\ndiv.coding_assumption span.assumption_attribution {\n  text-align: right;\n  grid-column: 11/21;\n  grid-row: 1;\n  /* grid-area: rule_attribution; */\n  width: 100%;\n  margin-right: 15px;\n  font-size: 0.95rem;\n}\n\ndiv.coding_assumption span.no_icons {\n  padding-top: 10px;\n  margin-left: -10px;\n}\n\ndiv.coding_assumption div.assumption_justifications {\n  grid-column: 1/10;\n  grid-row: 3;\n}\n\ndiv.coding_assumption div.assumption_justifications span {\n  display: grid;\n  place-items: center;\n}\ndiv.coding_assumption div.assumption_justifications li {\n  font-size: 0.75rem;\n  max-width: 45ch;\n}\n\ndiv.coding_assumption div.assumption_concrete_applications {\n  display: grid;\n  place-items: center;\n  grid-column: 11/21;\n  grid-row: 3;\n}\n\ndiv.coding_assumption div.assumption_concrete_applications span {\n}\n\ndiv.coding_assumption div.assumption_concrete_applications li {\n  font-size: 0.75rem;\n  max-width: 45ch;\n}\n\n/* #endregion CRs&As -- Coding Assumptions */\n\n/* #region Media Queries */\n\n@media screen and (min-width: 0px) and (max-width: 575px) {\n  main#coding_rules_and_assumptions h2 {\n    width: 95%;\n    font-size: 2rem;\n  }\n\n  div#container--coding_rules div.coding_rule,\n  div#container--coding_assumptions div.coding_assumption {\n    grid-template-rows: repeat(4, 25%);\n    width: 100%;\n  }\n\n  div.coding_rule div.rule_justifications,\n  div.coding_assumption div.assumption_justifications {\n    grid-column: 1/21;\n    grid-row: 3;\n  }\n\n  div.coding_rule div.rule_concrete_applications,\n  div.coding_assumption div.assumption_concrete_applications {\n    display: grid;\n    place-items: center;\n    grid-column: 1/21;\n    grid-row: 4;\n  }\n}\n\n/* #endregion Media Queries */\n"},"lineCount":null}},"error":null,"hash":"106da12e70d429d7a077eed8a0e9485e","cacheData":{"env":{}}}